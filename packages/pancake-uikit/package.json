{
  "name": "@pancakefork/uikit",
  "version": "0.43.8",
  "description": "Set of UI components for pancake projects",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "repository": "https://github.com/pancakeswap/pancake-toolkit/tree/master/packages/pancake-uikit",
  "private": false,
  "license": "MIT",
  "scripts": {
    "start": "yarn storybook",
    "build": "rd /s /q .\\dist && rollup -c && tsc -d --emitDeclarationOnly --declarationDir .\\dist",
    "lint": "eslint 'src/**/*.{js,jsx,ts,tsx}'",
    "format:check": "prettier --check --loglevel error 'src/**/*.{js,jsx,ts,tsx}'",
    "format:write": "prettier --write 'src/**/*.{js,jsx,ts,tsx}'",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook",
    "test": "jest",
    "prepublishOnly": "yarn build"
  },
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupTests.js"
    ]
  },
  "devDependencies": {
    "@open-wc/building-rollup": "^1.10.0",
    "@testing-library/jest-dom": "^5.11.6",
    "@testing-library/react": "^11.2.5",
    "@types/react-dom": "^17.0.5",
    "deepmerge": "^4.2.2",
    "jest-styled-components": "^7.0.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.56.0",
    "rollup-plugin-typescript": "^1.0.1",
    "styled-components": "^5.2.0",
    "tslib": "^2.3.0",
    "typescript": "^4.3.5"
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "styled-components": "^5.2.3"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "@rollup/plugin-typescript": "^8.2.5",
    "@rollup/plugin-url": "^6.1.0",
    "@types/lodash": "^4.14.168",
    "@types/styled-system": "^5.1.10",
    "lodash": "^4.17.20",
    "react-popper": "^2.2.5",
    "react-transition-group": "^4.4.1",
    "styled-system": "^5.1.5"
  },
  "publishConfig": {
    "access": "public"
  }
}
